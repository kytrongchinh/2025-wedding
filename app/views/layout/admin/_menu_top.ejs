<header class="main-header">
    <nav class="navbar navbar-static-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="<%= _adminUrl%>" class="navbar-brand"><b>ZSL</b>Admin</a>
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                    <i class="fa fa-bars"></i>
                </button>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse left pull-left" id="navbar-collapse">
                <ul class="nav navbar-nav">
                    <% admin_menu.forEach(function(menu){ %>
                    <% if(menu){ %>
                        <% if(menu.childs.length > 0){ %>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><%=menu.name%> <span class="caret"></span></a>
                                <ul class="dropdown-menu" role="menu">
                                    <% menu.childs.forEach(function(child,k){ %>
                                        <li><a href="<%= _baseUrl%><%=child.link%>"><span><%=child.name%></span></a></li>
                                        <%if(k < menu.childs.length-1){%><li class="divider"></li><%}%>
                                    <% }); %>
                                </ul>
                            </li>
                        <%}else{%>
                            <li>
                                <a href="<%= _baseUrl%><%=menu.link%>"><%=menu.name%></a>
                            </li>
                        <%}%>
                    <% } %>
                    <% }); %>
                </ul>
                <% if(helpers.admin.checkPermByResource('admin','dashboard','advanced-search',admin_userdata)){ %>
                <form method="get" action="<%=_baseUrl%>admin/dashboard/advanced-search" class="navbar-form navbar-left" role="search">
                    <div class="form-group">
                        <input type="text" name="advanced_search" class="form-control autocomplete-input" id="navbar-search-input" placeholder="Search" value="">
                    </div>
                </form>
                <%}%>
            </div>
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav navbar-right">
                    <!--Drop down notifications-->
                    <!-- <li class="dropdown notifications-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                            <i class="fa fa-bell-o"></i>
                            <span class="label label-warning">123</span>
                        </a>
                    </li> -->
                    <!--Drop down profile user-->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="<%=_staticUrl%><%=avatar%>" class="user-image" alt="User Image">
                            <span class="hidden-xs"><%=fullname%></span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <img src="<%=_staticUrl%><%=avatar%>" class="img-circle" alt="User Image">
                                <p><%=fullname%></p>
                            </li>
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="<%= _adminUrl%>/dashboard/update-profile" class="btn btn-info btn-flat">Edit Profile</a>
                                </div>
                                <div class="pull-right">
                                    <a href="<%= _adminUrl%>/logout" class="btn btn-info btn-flat">Logout</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="<%= _adminUrl%>" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>
<div class="clearfix"></div>