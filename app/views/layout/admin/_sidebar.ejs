<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="<%=_staticUrl%><%=avatar%>" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p><%= fullname%></p>
                <a href="<%= _adminUrl%>/logout"><i class="fa fa-circle text-success"></i>Logout</a>
            </div>
        </div>
        <ul class="sidebar-menu">
            <li class="header">MAIN NAVIGATION</li>
            <li class="">
                <a href="<%= _adminUrl%>">
                    <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                </a>
            </li>
            <% admin_menu.forEach(function(menu){ %>
            <% if(menu){ %>
            <li class="">
                <a href="<%= _baseUrl%><%=menu.link%>">
                    <i class="fa <%=menu.icon%>"></i>
                    <span><%=menu.name%></span>
                    <i class="fa <% if(menu.childs.length > 0){ %> fa-angle-left pull-right <%}%>"></i>
                </a>
                <% if(menu.childs.length > 0){ %>
                    <ul class="treeview-menu">
                        <% menu.childs.forEach(function(child){ %>
                            <li> <a href="<%= _baseUrl%><%=child.link%>"><i class="fa <%=child.icon%>"></i><span><%=child.name%></span></a></li>
                        <% }); %>
                    </ul>
                <%}%>
            </li>
            <% } %>
        <%}); %>
        </ul>

    </section>
    <!-- /.sidebar -->
</aside>