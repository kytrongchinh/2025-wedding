<header class="main-header">
    <!-- Logo -->
    <a href="#" class="logo">
        <span class="logo-mini">ZSL</span>
        <span class="logo-lg"><b>Admin</b>ZSL</span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top" role="navigation">
        <!-- Sidebar toggle button-->
        <a role="button" class="sidebar-toggle" data-toggle="offcanvas"><i class="fa fa-bars"></i></a>

        

        <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
                <!-- User Account: style can be found in dropdown.less -->
                <li class="dropdown user user-menu">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <img src="<%=_staticUrl%><%=avatar%>" class="user-image" alt="User Image">
                        <span class="hidden-xs"><%=fullname%></span>
                    </a>
                    <ul class="dropdown-menu">
                        <!-- User image -->
                        <li class="user-header">
                            <img src="<%=_staticUrl%><%=avatar%>" class="img-circle" alt="User Image">
                        </li>
                        <li class="user-footer">
                            <div class="pull-left">
                                <a href="<%= _adminUrl%>/dashboard/update-profile" class="btn btn-info btn-flat">Update Profile</a>
                            </div>
                            <div class="pull-right">
                                <a href="<%= _adminUrl%>/logout" class="btn btn-info btn-flat">Logout</a>
                            </div>
                        </li>
                    </ul>
                </li>
                <!-- Control Sidebar Toggle Button -->
                <li>
                    <a href="<%= _adminUrl%>" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                </li>
            </ul>
            
        </div>
    </nav>
</header>
<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="<%=_staticUrl%><%=avatar%>" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p><%= fullname%></p>
                <a href="<%= _adminUrl%>/logout"><i class="fa fa-circle text-success"></i>Logout</a>
            </div>
        </div>
        
        <ul class="sidebar-menu">
            <li class="header">
                <% if(helpers.admin.checkPermByResource('admin','dashboard','advanced-search',admin_userdata)){ %>
                    <div class="form-group">
                        <input type="text" name="advanced_search" class="form-control autocomplete-input" id="navbar-search-input" placeholder="Search" value="">
                    </div>
                <%}%>
            </li>
            <li class="header">MAIN NAVIGATION</li>
            <li class="">
                <a href="<%= _adminUrl%>">
                    <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                </a>
            </li>
            <% admin_menu.forEach(function(menu){ %>
            <% if(menu){ %>
            <li class="">
                <a href="<%= _baseUrl%><%=menu.link%>">
                    <i class="fa <%=menu.icon%>"></i>
                    <span><%=menu.name%></span>
                    <i class="fa <% if(menu.childs.length > 0){ %> fa-angle-left pull-right <%}%>"></i>
                </a>
                <% if(menu.childs.length > 0){ %>
                    <ul class="treeview-menu">
                        <% menu.childs.forEach(function(child){ %>
                            <li> <a href="<%= _baseUrl%><%=child.link%>"><i class="fa <%=child.icon%>"></i><span><%=child.name%></span></a></li>
                        <% }); %>
                    </ul>
                <%}%>
            </li>
            <% } %>
        <%}); %>
        </ul>
    </section>
    <!-- /.sidebar -->
</aside>